<div class="movie">
    <h2>Title: {{title}}</h2>
    <img
        src="{{imageUrl}}">
    <p>Available Tickets: {{tickets}}</p>
    <div class="btn-group">
        <button><a href="#/movies/edit/{{_id}}">Edit</a></button>
        <button><a href="#/movies/delete/{{_id}}">Delete</a></button>
        <button><a href="#/buyTicket/{{_id}}">Buy Ticket</a></button>
        <button><a href="#/movies/details/{{_id}}">Details</a></button>
    </div>
</div>